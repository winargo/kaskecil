                                            option(value=''+resu.category_name) #{resu.category_name}
                                        else
                                          option(value=''+a.expense_category selected) #{a.expense_category}
                                          each resu in listcategory
                                            if(a.expense_category!=[resu.category_name])
                                              option(value=resu.category_name) #{resu.category_name}

