                                      select.custom-select.form-control(id='editselectname'+index style='width:338px;display:inline-block' onchange='editcategname('+index+')' name='editcategory' title="Select your category") 
                                          
                                        if(a.expense_category==null) 
                                          option(value='0' selected='') Select
