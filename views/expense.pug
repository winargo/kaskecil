                rows.push([response.data.dataaccount[j]["account_name"], response.data.dataaccount[j]["account_fullcurrency"].substring(4)+" "+response.data.dataaccount[j]["account_balance_current"],
                response.data.dataaccount[j]["account_category"], response.data.dataexpense[i]["expense_to"], response.data.dataaccount[j]["account_fullcurrency"].substring(4)+" "+response.data.dataexpense[i]["expense_amount"], response.data.dataexpense[i]["expense_date"], 
                "<button data-toggle='modal' data-target="+'#edit'+response.data.dataexpense[i]["expense_id"]+  " class='btn btn-default'><i class='fa fa-pencil'></i></button><button  data-toggle='modal' data-target="+'#delete'+response.data.dataexpense[i]["expense_id"]+  " class='btn btn-default'><i class='fa fa-trash'></i></button>"])
              }
            }
          }
          
          $('#main_table').DataTable( {
              data: rows,
              dom: 'Bfrtip',
              columnDefs: [ {
                "targets": 6,
                "orderable": false,
                "className":"text-center"
              },
              {
                "targets": [0,2,3,5],
                "className":"text-center"
              },
              {
                "targets": [1,4],
                "className":"text-right"
              }
              ],
              lengthMenu: [
                [ 10, 25, 50, -1 ],
                [ '10 rows', '25 rows', '50 rows', 'Show all' ]
              ],
              //- buttons: [
                  
              //- ]
              buttons: [
                  'pageLength',
                  {
                      extend: 'pdfHtml5',
                      download: 'open'
                  }
              ]
          } );
        })
        .catch(function (error) {
          console.log(error);
        });
      })
      .catch(function (error) {
        console.log(error);
      });


